{
  "feature_extractor": {
    "name": "basic_netflow",
    "parameters": {
      "--nf-mode": [
        [
          "subflows_simple",
          "tcp",
          "with_ip"
        ]
      ],
      "--flow-timeout": [
        12,
        20,
        60
      ],
      "--subflow-timeout": [
        0.5,
        2
      ]
    }
  },
  "decision_engine": {
    "name": "autoencoder",
    "parameters": {
      "--training-epochs": [
        50,
        200,
        400
      ],
      "--training-batch": [
        64
      ],
      "--layers": [
        "*0.8,*0.8,#1",
        "*0.6,*0.8,*0.8,*0.9,#3,#2,#1",
        "*1.2,*1,2,#1",
        "*2",
        "*0.5,*0.5,#1"
      ],
      "--activation": [
        "relu",
        "sigmoid"
      ],
      "--loss": [
        "mse",
        "mae"
      ]
    }
  },
  "transformers": [
    "onehot_encoder",
    "minmax_scaler",
    "standard_scaler"
  ],
  "mode": "grid_search"
}
