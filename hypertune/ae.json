{
  "feature_extractor": {
    "name": "flows",
    "parameters": {
      "--nf-mode": [
        [
          "subflows_simple",
          "with_ip"
        ],
        [
          "tcp",
          "subflows_simple",
          "with_ip"
        ]
      ],
      "--flow-timeout": [
        12
      ],
      "--subflow-timeout": [
        0.5
      ]
    }
  },
  "decision_engine": {
    "name": "autoencoder",
    "parameters": {
      "--training-epochs": [
        400
      ],
      "--training-batch": [
        256
      ],
      "--layers": [
        "*0.8,*0.8,#1",
        "*0.6,*0.8,*0.8,*0.9,#3,#2,#1",
        "*2",
        "*2,*1.5,*1.5,#2,#1",
        "50",
        "*0.5",
        "300,200,75,10,75,200,300"
      ],
      "--activation": [
        "relu",
        "sigmoid"
      ],
      "--loss": [
        "mse",
        "mae"
      ]
    }
  },
  "transformers": [
    "onehot_encoder",
    "minmax_scaler",
    "standard_scaler"
  ],
  "mode": "grid_search"
}
