{
  "feature_extractor": {
    "name": "flow_extractor",
    "parameters": {
      "--nf-mode": [
        [
          "subflows_simple",
          "tcp",
          "with_ip_addr"
        ]
      ],
      "--flow-timeout": [
        12
      ],
      "--subflow-timeout": [
        0.5
      ],
      "--w2v-window-size": [
        5,
        10
      ],
      "--w2v-group-bytes": [
        1,
        2,
        4,
        8,
        16
      ],
      "--w2v-min-count": [
        1,
        2
      ],
      "--w2v-training-method": [
        "cbow",
        "skip_gram"
      ],
      "--w2v-vector-size": [
        10,
        20,
        50
      ]
    }
  },
  "decision_engine": {
    "name": "autoencoder",
    "parameters": {
      "--training-epochs": [
        200
      ],
      "--training-batch": [
        256
      ],
      "--layers": [
        "*0.8,*0.8,#1"
      ],
      "--activation": [
        "relu"
      ],
      "--loss": [
        "mse"
      ]
    }
  },
  "transformers": [
    "onehot_encoder",
    "minmax_scaler",
    "standard_scaler"
  ],
  "mode": "grid_search"
}
