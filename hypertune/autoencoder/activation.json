{
  "feature_extractor": {
    "name": "flow_extractor",
    "parameters": {
      "--nf-mode": [
        [
          "tcp",
          "subflows"
        ]
      ],
      "--flow-timeout": [
        12
      ],
      "--subflow-timeout": [
        0.5
      ]
    }
  },
  "decision_engine": {
    "name": "autoencoder",
    "parameters": {
      "--training-epochs": [
        400
      ],
      "--training-batch": [
        1024
      ],
      "--layers": [
        "*0.9,*0.8,*0.8,#2,#1",
        "300,200,75,10,75,200,300",
        "200,20,200",
        "400,200,10,200,400"
      ],
      "--activation": [
        "relu",
        "sigmoid",
        "tanh",
        "exponential",
        "softmax"
      ],
      "--loss": [
        "mse",
        "mae"
      ],
      "--threshold": [
        99.99,
        99.0
      ]
    }
  },
  "transformers": [
    "onehot_encoder",
    "minmax_scaler",
    "standard_scaler"
  ],
  "mode": "grid_search"
}
