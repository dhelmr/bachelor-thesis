{
  "feature_extractor": {
    "name": "flow_extractor",
    "parameters": {
      "--nf-mode": [
        [
          "tcp",
          "subflows",
          "with_ip_addr",
          "port_categorial",
          "hindsight"
        ]
      ],
      "--flow-timeout": [
        12
      ],
      "--subflow-timeout": [
        0.5
      ]
    }
  },
  "decision_engine": {
    "name": "autoencoder",
    "parameters": {
      "--training-epochs": [
        400
      ],
      "--training-batch": [
        64
      ],
      "--layers": [
        "*0.8,*0.8,#1",
        "*2,*1.5,*1.5,#2,#1",
         "300,200,75,10,75,200,300"
      ],
      "--activation": [
        "relu"
      ],
      "--loss": [
        "mse"
      ]
    }
  },
  "transformers": [
    "onehot_encoder",
    "minmax_scaler",
    "standard_scaler"
  ],
  "mode": "grid_search"
}
