{
  "feature_extractor": {
    "name": "flow_extractor",
    "parameters": {
      "--nf-mode": [
        [
          "tcp",
          "subflows_simple",
          "with_ip_addr"
        ]
      ],
      "--flow-timeout": [
        12
      ],
      "--subflow-timeout": [
        0.5
      ]
    }
  },
  "decision_engine": {
    "name": "autoencoder",
    "parameters": {
      "--training-epochs": [
        400
      ],
      "--training-batch": [
        64
      ],
      "--layers": [
        "*0.9,*0.8,*0.8,#2,#1",
        "300,200,75,10,75,200,300"
      ],
      "--activation": [
        "relu"
      ],
      "--loss": [
        "mse"
      ],
      "--dropout-ratio": [
        0.05,
        0.1,
        0.2,
        0.3,
        0.7,
        0.8
      ],
      "--batch-normalization": [
        "none",
        "after_activation"
      ],
      "--early-stopping-patience": [
        3
      ]
    }
  },
  "transformers": [
    "onehot_encoder",
    "minmax_scaler",
    "standard_scaler"
  ],
  "mode": "grid_search"
}
