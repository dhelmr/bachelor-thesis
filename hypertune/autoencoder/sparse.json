{
  "feature_extractor": {
    "name": "flow_extractor",
    "parameters": {
      "--nf-mode": [
        [
          "tcp",
          "subflows_simple",
          "with_ip"
        ]
      ],
      "--flow-timeout": [
        12
      ],
      "--subflow-timeout": [
        0.5
      ]
    }
  },
  "decision_engine": {
    "name": "autoencoder",
    "parameters": {
      "--training-epochs": [
        400
      ],
      "--training-batch": [
        64
      ],
      "--layers": [
        "*4,*0.1,#1",
        "512",
        "*2,100,#1",
        "*0.5,*5,#1"
      ],
      "--activation": [
        "sigmoid"
      ],
      "--loss": [
        "mse"
      ],
      "--dropout-ratio": [
        0.1
      ],
      "--batch-normalization": [
        "none",
        "after_activation"
      ],
      "--early-stopping-patience": [
        3
      ]
    }
  },
  "transformers": [
    "onehot_encoder",
    "minmax_scaler",
    "standard_scaler"
  ],
  "mode": "grid_search"
}
