{
  "feature_extractor": {
    "name": "flow_extractor",
    "parameters": {
      "--nf-mode": [
        "basic",
        ["tcp", "basic"],
        [
          "tcp",
          "subflows"
        ],
        [
          "tcp",
          "subflows",
          "with_ip_addr"
        ],
        [
          "tcp",
          "subflows",
          "with_ip_addr",
          "ip_dotted"
        ],
        [
          "tcp",
          "subflows",
          "port_decimal"
        ],
        [
          "tcp",
          "subflows",
          "hingsight"
        ]
      ],
      "--flow-timeout": [
        6,12,60
      ],
      "--subflow-timeout": [
        0.2, 0.5, 1.0
      ]
    }
  },
  "decision_engine": {
    "name": "autoencoder",
    "parameters": {
      "--training-epochs": [
        200
      ],
      "--training-batch": [
        1024
      ],
      "--layers": [
        "*0.9,*0.8,*0.8,#2,#1"
      ],
      "--activation": [
        "relu"
      ],
      "--loss": [
        "mse"
      ]
    }
  },
  "transformers": [
    "onehot_encoder",
    "minmax_scaler",
    "standard_scaler"
  ],
  "mode": "grid_search"
}
