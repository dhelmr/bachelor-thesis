{
  "feature_extractor": {
    "name": "flow_extractor",
    "parameters": {
      "--nf-mode": [
        [
          "subflows_detailed",
          "tcp",
          "subflows_simple",
          "with_ip_addr"
        ],
        [
          "tcp",
          "subflows_simple",
          "with_ip_addr"
        ],
        [
          "tcp",
          "subflows_simple"
        ],
        [
          "tcp",
          "with_ip_addr"
        ],
        "tcp",
        "none"
      ],
      "--flow-timeout": [
        6,
        12
      ],
      "--subflow-timeout": [
        0.5,
        1
      ]
    }
  },
  "decision_engine": {
    "name": "autoencoder",
    "parameters": {
      "--training-epochs": [
        20
      ],
      "--training-batch": [
        64
      ],
      "--layers": [
        "*0.9,*0.8,*0.8,#2,#1"
      ],
      "--activation": [
        "sigmoid"
      ],
      "--loss": [
        "mse"
      ],
      "--dropout-ratio": [
        0.1
      ],
      "--batch-normalization": [
        "after_activation"
      ],
      "--early-stopping-patience": [
        3
      ]
    }
  },
  "transformers": [
    "onehot_encoder",
    "minmax_scaler",
    "standard_scaler"
  ],
  "mode": "grid_search"
}
