{
  "feature_extractor": {
    "name": "flow_extractor",
    "parameters": {
      "--nf-mode": [
        [
          "tcp",
          "subflows",
          "with_ip_addr"
        ]
      ],
      "--flow-timeout": [
        1,
        2,
        6,
        8,
        10,
        11,
        12,
        16,
        20,
        60
      ],
      "--subflow-timeout": [
        0.4,
        0.45,
        0.5,
        0.55,
        0.6,
        0.8,
        1
      ]
    }
  },
  "decision_engine": {
    "name": "autoencoder",
    "parameters": {
      "--training-epochs": [
        20
      ],
      "--training-batch": [
        64
      ],
      "--layers": [
        "*0.9,*0.8,*0.8,#2,#1"
      ],
      "--activation": [
        "sigmoid"
      ],
      "--loss": [
        "mse"
      ],
      "--dropout-ratio": [
        0.1
      ],
      "--batch-normalization": [
        "after_activation"
      ],
      "--early-stopping-patience": [
        3
      ]
    }
  },
  "transformers": [
    "onehot_encoder",
    "minmax_scaler",
    "standard_scaler"
  ],
  "mode": "grid_search"
}
