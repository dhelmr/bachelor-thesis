{
  "feature_extractor": {
    "name": "flow_extractor",
    "parameters": {
      "--nf-mode": [
        [
          "subflows",
          "tcp",
          "with_ip_addr"
        ]
      ],
      "--flow-timeout": [
        20,
        60
      ],
      "--subflow-timeout": [
        2
      ]
    }
  },
  "decision_engine": {
    "name": "local_outlier_factor",
    "parameters": {
      "--metric": [
        "minkowski"
      ],
      "--minkowski-p": [
        1,
        1.5,
        2,
        5,
        0.5
      ],
      "--algorithm": [
        "ball_tree",
        "kd_tree",
        "brute"
      ],
      "--n-neighbors": [
        5,
        20,
        80
      ]
    }
  },
  "transformers": [
    "onehot_encoder",
    "minmax_scaler",
    "standard_scaler"
  ],
  "mode": "grid_search"
}
