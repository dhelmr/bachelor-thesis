{
  "feature_extractor": {
    "name": "basic_netflow",
    "parameters": {
      "--nf-mode": [
        "subflows_simple",
        "with_ip",
        "tcp",
        [
          "subflows_simple",
          "subflows_detailed"
        ],
        [
          "subflows_simple",
          "subflows_detailed",
          "tcp",
          "with_ip"
        ]
      ],
      "--flow-timeout": [
        6,
        10,
        12,
        20,
        60
      ],
      "--subflow-timeout": [
        0.5,
        1,
        1.5,
        2
      ]
    }
  },
  "decision_engine": {
    "name": "one_class_svm",
    "parameters": {
      "--gamma": [
        0.005,
        0.01,
        0.02,
        0.05,
        0.1,
        0.2
      ],
      "--shrinking": [
        "True",
        "False"
      ],
      "--kernel": [
        "rbf",
        "poly",
        "sigmoid"
      ]
    }
  },
  "transformers": [
    "onehot_encoder",
    "minmax_scaler",
    "standard_scaler"
  ],
  "mode": "grid_search"
}