{
  "feature_extractor": {
    "name": "basic_netflow",
    "parameters": {
      "--nf-mode": [
        [
          "subflows_simple",
          "tcp",
          "with_ip"
        ]
      ],
      "--flow-timeout": [
        12,
        20,
        60
      ],
      "--subflow-timeout": [
        2
      ]
    }
  },
  "decision_engine": {
    "name": "one_class_svm",
    "parameters": {
      "--gamma": [
        0.01,
        0.02,
        0.05,
        0.1,
        0.2,
        0.005
      ],
      "--kernel": [
        "rbf",
        "poly",
        "sigmoid"
      ]
    }
  },
  "transformers": [
    "onehot_encoder",
    "minmax_scaler",
    "standard_scaler"
  ],
  "mode": "grid_search"
}