{
  "feature_extractor": {
    "name": "flows",
    "parameters": {
      "--nf-mode": [
        [
          "subflows_simple",
          "tcp",
          "with_ip"
        ]
      ],
      "--flow-timeout": [
        12
      ],
      "--subflow-timeout": [
        0.5
      ]
    }
  },
  "decision_engine": {
    "name": "one_class_svm",
    "parameters": {
      "--gamma": [
        0.005,
        0.01
      ],
      "--kernel": [
        "rbf"
      ],
      "--nu": [
        0.001,
        0.0001,
        0.000001
      ]
    }
  },
  "transformers": [
    "onehot_encoder",
    "minmax_scaler",
    "standard_scaler"
  ],
  "mode": "grid_search"
}