{
  "feature_extractor": {
    "name": "flow_extractor",
    "parameters": {
      "--nf-mode": [
        [
          "subflows",
          "tcp",
          "with_ip_addr"
        ]
      ],
      "--flow-timeout": [
        12
      ],
      "--subflow-timeout": [
        0.5
      ]
    }
  },
  "decision_engine": {
    "name": "one_class_svm",
    "parameters": {
      "--gamma": [
        0.005,
        0.001,
        0.002,
        0.0001
      ],
      "--kernel": [
        "rbf",
        "linear"
      ],
      "--nu": [
        0.0005,
        0.0002,
        0.0001,
        0.00005,
        0.00001
      ]
    }
  },
  "transformers": [
    "onehot_encoder",
    "minmax_scaler",
    "standard_scaler"
  ],
  "mode": "grid_search"
}