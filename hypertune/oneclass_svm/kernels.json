{
  "feature_extractor": {
    "name": "flow_extractor",
    "parameters": {
      "--nf-mode": [
        [
          "subflows",
          "tcp",
          "with_ip_addr"
        ]
      ],
      "--flow-timeout": [
        12
      ],
      "--subflow-timeout": [
        0.5
      ]
    }
  },
  "decision_engine": {
    "name": "one_class_svm",
    "parameters": {
      "--gamma": [
        0.005
      ],
      "--kernel": [
        "poly",
        "sigmoid"
      ],
      "--nu": [
        0.0001
      ],
      "--coef0": [
        0.0,
        0.1,
        0.2,
        1
      ]
    }
  },
  "transformers": [
    "onehot_encoder",
    "minmax_scaler",
    "standard_scaler"
  ],
  "mode": "grid_search"
}