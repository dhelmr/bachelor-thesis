{
  "feature_extractor": {
    "name": "payl_flows",
    "parameters": {
      "--nf-mode": [
        null,
        "tcp"
      ],
      "--flow-timeout": [
        12
      ],
      "--subflow-timeout": [
        0.5
      ],
      "--smoothing": [
        0.001, 0.1, 0.000001, 0.0001
      ],
      "--clustering-threshold": [
        0, 32, 64, 128, 256, 1024, 32768
      ]
    }
  },
  "decision_engine": {
    "name": "autoencoder",
    "parameters": {
      "--training-epochs": [
        200
      ],
      "--training-batch": [
        64
      ],
      "--layers": [
        "*0.9,*0.8,*0.8,#2,#1"
      ],
      "--activation": [
        "sigmoid"
      ],
      "--loss": [
        "mse"
      ]
    }
  },
  "transformers": [
    "onehot_encoder",
    "minmax_scaler",
    "standard_scaler"
  ],
  "mode": "grid_search"
}
