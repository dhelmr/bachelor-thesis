{
  "feature_extractor": {
    "name": "payl_flows",
    "parameters": {
      "--nf-mode": [
        ["basic",
        "tcp"],
        ["tcp", "subflows"]
      ],
      "--flow-timeout": [
        1,6,12
      ],
      "--subflow-timeout": [
        0.5
      ],
      "--smoothing": [
        0.1
      ],
      "--clustering-threshold": [
        0.5, 2, 4, 5, 7, 20
      ]
    }
  },
 "decision_engine": {
    "name": "one_class_svm",
    "parameters": {
      "--gamma": [
        0.005
      ],
      "--kernel": [
        "rbf",
        "sigmoid"
      ],
      "--nu": [
        0.0001
      ]
    }
  },
  "transformers": [ [
    "onehot_encoder",
    "minmax_scaler",
    "standard_scaler"
  ]
  ],
  "mode": "grid_search"
}
