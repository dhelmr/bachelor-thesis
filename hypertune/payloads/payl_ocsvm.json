{
  "feature_extractor": {
    "name": "payl_flows",
    "parameters": {
      "--nf-mode": [
        null,
        "tcp"
      ],
      "--flow-timeout": [
        6,
        12
      ],
      "--subflow-timeout": [
        0.5
      ],
      "--smoothing": [
        0.001, 0.1, 0.000001, 0.0001
      ],
      "--clustering-threshold": [
        0.5, 2, 4, 5, 7, 20
      ]
    }
  },
 "decision_engine": {
    "name": "one_class_svm",
    "parameters": {
      "--gamma": [
        0.005
      ],
      "--kernel": [
        "sigmoid"
      ],
      "--nu": [
        0.0001
      ]
    }
  },
  "transformers": [ [
    "onehot_encoder",
    "minmax_scaler",
    "standard_scaler"
  ],
    [
    "minmax_scaler",
    "standard_scaler"
  ]
  ],
  "mode": "grid_search"
}
