{
  "feature_extractor": {
    "name": "flow_extractor",
    "parameters": {
      "--nf-mode": [
        [
          "tcp",
          "subflows",
          "with_ip_addr"
        ]
      ],
      "--flow-timeout": [
        12
      ],
      "--subflow-timeout": [
        0.5
      ]
    }
  },
  "decision_engine": {
    "name": "autoencoder",
    "parameters": {
      "--training-epochs": [
        400
      ],
      "--training-batch": [
        64
      ],
      "--layers": [
        "*0.8,*0.8,*0.8,#2,#1",
        "*0.9,*0.8,*0.8,#2,#1",
        "300,200,75,10,75,200,300"
      ],
      "--activation": [
        "sigmoid"
      ],
      "--loss": [
        "mse"
      ]
    }
  },
  "transformers": [
    [
      "onehot_encoder",
      "minmax_scaler",
      "standard_scaler"
    ],
    [
      "onehot_encoder",
      "standard_scaler"
    ],
    [
      "onehot_encoder",
      "minmax_scaler"
    ],
    [
      "minmax_scaler"
    ],
    [
      "standard_scaler"
    ],
    []
  ],
  "mode": "grid_search"
}
