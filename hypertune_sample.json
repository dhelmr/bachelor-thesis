{
  "feature_extractor": {
    "name": "basic_netflow",
    "parameters": {
      "--mode": [
        "subflows_simple"
      ],
      "--flow-timeout": [
        1000,
        6000,
        8000,
        10000,
        12000,
        20000
      ],
      "--subflow-timeout": [
        100,
        500,
        1000,
        2000
      ]
    }
  },
  "decision_engine": {
    "name": "one_class_svm",
    "parameters": {
      "--gamma": [
        0.005,
        0.01,
        0.02,
        0.05,
        0.1,
        0.2
      ],
      "--shrinking": [
        "True",
        "False"
      ],
      "--kernel": [
        "rbf",
        "poly",
        "sigmoid"
      ]
    }
  },
  "transformers": [
    "minmax_scaler",
    "standard_scaler"
  ],
  "mode": "grid_search"
}